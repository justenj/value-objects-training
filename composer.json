    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Justenj\\ValueObjects\\": "src"
        }
    },
    "autoload-dev": {
